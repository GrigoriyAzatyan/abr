### Формат: export ПЕРЕМЕННАЯ=значение ###

#Пароль PostgreSQL:
export PG_PASS=

#IP кластера PostgreSQL:
export PG_HOST=

#DNS-имя сервера TrueConf:
export TRUECONF_DNS_NAME=

#Client_id API TrueConf сервера:
export TRUECONF_CLIENT_ID=

#Client_secret API TrueConf сервера:
export TRUECONF_CLIENT_SECRET=

#DNS-имя сервера Ростелл:
export ROSTELL_DNS_NAME=rostell.domain.com

#Коммуникационный домен Ростелл:
export ROSTELL_COMM_DOMAIN=

#Учетка Ростелл с доступом к API:
export ROSTELL_LOGIN=

#Пароль учетки Ростелл:
export ROSTELL_PASS=

#IP контроллера домена LDAP:
export LDAP_DC=

#Краткое имя домена LDAP (префикс для учеток):
export LDAP_DOMAIN=

#Учетка для доступа в LDAP:
export LDAP_LOGIN=

#Пароль учетки LDAP:
export LDAP_PASS=
